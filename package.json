{
  "name": "gulp-boilerplate",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "yarn dev",
    "dev": "gulp build:dev",
    "prebuild": "rimraf ./production",
    "build": "cross-env NODE_ENV=production gulp build",
    "browser": "browser-sync start -s 'production'",
    "iconfont": "gulp iconfont:build && yarn lint:style",
    "lint:html": "gulp pug:lint",
    "lint:script": "eslint --fix --ext .ts --ignore-path .eslintignore .",
    "lint:style": "stylelint --fix --ignore-path .stylelintignore \"**/*.scss\"",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": ["yarn lint:script", "git add --force"],
    "*.scss": ["yarn lint:style", "git add --force"]
  },
  "author": "kota",
  "license": "ISC",
